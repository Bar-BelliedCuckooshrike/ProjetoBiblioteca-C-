<Window x:Class="BibliotecaWPF.Views.MenuUsuario"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BibliotecaWPF.Views"
        mc:Ignorable="d"
        Title="MenuUsuario" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtConfirmLogin" HorizontalAlignment="Left" Margin="184,260,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Menu Margin="10,10,10,0" Padding="10,0,0,0" VerticalAlignment="Top" Height="34">
            <Button x:Name="btnCadastrarLivro" Content="Cadastrar livro" HorizontalAlignment="Left" VerticalAlignment="Top" Click="btnCadastrarLivro_Click"/>
            <Button x:Name="btnListarLivros" Content="Listar livros" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75"/>
        </Menu>

    </Grid>
</Window>
